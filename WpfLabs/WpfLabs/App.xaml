                <ResourceDictionary Source="CustomWindow/Style.Window.Universal.xaml" />
                <!--<ResourceDictionary Source="MasterDetailDataGrid/Style/DataGridNormalStyle.xaml"></ResourceDictionary>-->
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
